/[0-9]+/ {
	var err error
	lval.ival, err = strconv.Atoi(yylex.Text())
	if err != nil {
		yylex.Error(err.Error())
	}
	return tNumber
}

/struct/ {
	return tStruct
}

/u?int(8|16|24|32|64)/ {
	var err error
	lval.n, err = ast.ParseIntegerType(yylex.Text())
	if err != nil {
		yylex.Error(err.Error())
	}
	return tIntegerType
}

/[{}\[\]<>,]/ {
	return int(yylex.Text()[0])
}

/[a-zA-Z_]+[0-9a-zA-Z_]/ {
	lval.sval = yylex.Text()
	return tIdentifier
}

/[ \t\n]+/        { /* eat up whitespace */ }
/./               { println("Unrecognized character:", yylex.Text()) }
//
package parse

import (
	"strconv"

	"framecc/ast"
)
